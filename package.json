    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.1",
    "prisma": "^4.8.0",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "apollo-server": "^3.8.2",
    "chai": "^4.3.6",
    "graphql": "^16.5.0",
    "graphql-import-node": "^0.0.5",
    "mocha": "^10.0.0",
    "supertest": "^6.3.0"
  },
  "packageManager": "yarn@3.2.4"
